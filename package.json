  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist docs node_modules",
    "setup": "npm install && npm audit fix",
    "build": "rm -rf dist docs && tsc -p tsconfig.build.json && npx typedoc",
    "check": "npx vitest --typecheck run",
    "watch": "npx tsc --watch",
    "proof": "npx typedoc && (npx typedoc --watch & npx five-server docs --open)"
  },
  "devDependencies": {
    "five-server": "^0.4.5",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
